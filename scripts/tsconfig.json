    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": [ "es5", "es6", "dom" ],
